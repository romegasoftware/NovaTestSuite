{
    "name": "romegadigital/nova-test-suite",
    "description": "Library to help testing laravel nova resources faster",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Krishan Koenig",
            "email": "krishan.koenig@googlemail.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.1",
        "illuminate/filesystem": "^7.1",
        "illuminate/testing": "^7.1",
        "illuminate/contracts": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Romegadigital\\NovaTestSuite\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Romegadigital\\NovaTestSuite\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Romegadigital\\NovaTestSuite\\NovaTestSuiteServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^v5.0.2",
        "dms/phpunit-arraysubset-asserts": "^0.1.0"
    }
}
